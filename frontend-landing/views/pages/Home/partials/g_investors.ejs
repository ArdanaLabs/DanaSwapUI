<div class="home-investor-bg">
    <div class="Container-root Container-maxWidthMd">
        <h3 class="Typography-root home-investor-title Typography-h3">
            Investors
        </h3>
        <div class="home-investor-logos">
            <% let logos = [
                {
                    href: "https://www.threearrowscap.com/select-investments/",
                    img_src: "../../../../assets/logos/three-arrow-capital.svg",
                    img_alt: "Three Arrow Capital"
                }, {
                    href: "https://cfund.vc/",
                    img_src: "../../../../assets/logos/cfund.svg",
                    img_alt: "cFund"
                }, {
                    href: "https://ascensiveassets.com/",
                    img_src: "../../../../assets/logos/ascentive-assets.svg",
                    img_alt: "Ascensive Assets"
                }, {
                    href: "https://kronosresearch.com/",
                    img_src: "../../../../assets/logos/kronos.svg",
                    img_alt: "Kronos Research"
                }, {
                    href: "https://www.morningstar.ventures/",
                    img_src: "../../../../assets/logos/morning-star.svg",
                    img_alt: "Morningstar Ventures"
                }, {
                    href: "https://www.mechanism.capital/",
                    img_src: "../../../../assets/logos/mechanism-capital.svg",
                    img_alt: "Mechanism.Capital"
                }, {
                    href: "https://qcp.capital/",
                    img_src: "../../../../assets/logos/QCP-capital.svg",
                    img_alt: "QCP Capital"
                }, {
                    href: "https://www.iangels.com/",
                    img_src: "../../../../assets/logos/angels.svg",
                    img_alt: "iAngels"
                }
            ] %>
            <% logos.forEach(function(logo){ %>
                <%- include('../../../components/logo.ejs', {logo: logo}) %>
            <% }); %>
            <% let posters = [{
                src: "../../../../assets/logos/david-post.svg",
                alt: "David Post"
            }, {
                src: "../../../../assets/logos/taiyang-zhang.svg",
                alt: "Taiyang Zhang"
            }, {
                src: "../../../../assets/logos/justin-sun.svg",
                alt: "Justin Sun"
            }] %>
            <% posters.forEach(function(poster){ %>
                <div class="brandLogo-flex">
                    <img class="logo-img" src="<%=poster.src%>" alt="<%=poster.alt%>" />
                </div>
            <% }); %>
        </div>
    </div>
</div>
